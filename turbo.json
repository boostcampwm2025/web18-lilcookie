{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", ".output/**"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "backend#dev": {
      "dependsOn": ["^build", "prisma:generate"],
      "cache": false,
      "persistent": true
    },
    "prisma:generate": {
      "cache": false
    },
    "lint": {},
    "test": {}
  }
}
