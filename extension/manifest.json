{
  "manifest_version": 3,
  "name": "TeamStash",
  "version": "1.0",
  "description": "한 번의 클릭으로 URL을 저장하는 확장 프로그램",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "host_permissions": ["https://link-repository.eupthere.com/*"],
  "permissions": ["activeTab", "storage", "alarms", "notifications"]
}
